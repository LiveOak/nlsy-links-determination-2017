            <TabItem Header="Gen2O-Math" x:Name="tabGen2OutcomesMath"  >
                <DataGrid />
            </TabItem>
            <TabItem Header="Item" x:Name="tabItem" >
                <DataGrid ItemsSource="{Binding Source={StaticResource tblItemViewSource1}}" />
            </TabItem>
            <TabItem Header="Variable" x:Name="tabVariable" >
                <DataGrid ItemsSource="{Binding Source={StaticResource tblVariableViewSource1}}" />
            </TabItem>
            <TabItem Header="MzManual" x:Name="tabMzManual" >
